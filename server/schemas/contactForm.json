{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "name",
        "email",
        "phone",
        "message"
    ],
    "properties": {
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "Jane Doe"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "mail@mail.com"
            ]
        },
        "phone": {
            "type": "string",
            "default": "",
            "title": "The phone Schema",
            "examples": [
                "1234567899"
            ]
        },
        "message": {
            "type": "string",
            "default": "",
            "title": "The message Schema",
            "examples": [
                "Hi.  Do you offer travel to moons?"
            ]
        }
    },
    "examples": [
        {
            "name": "Jane Doe",
            "email": "mail@mail.com",
            "phone": "1234567899",
            "message": "Hi.  Do you offer travel to moons?"
        }
    ]
}