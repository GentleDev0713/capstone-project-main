{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Auth Login Schema",
    "required": [
        "username",
        "password"
    ],
    "properties": {
        "username": {
            "type": "string",
            "minLength": 6,
            "maxLength": 15,
            "title": "The username Schema",
            "examples": [
                "sampleusername"
            ]
        },
        "password": {
            "type": "string",
            "minLength": 8,
            "maxLength": 20,
            "title": "The password Schema",
            "examples": [
                "samplepassword"
            ]
        }
    },
    "examples": [
        {
            "username": "sampleusername",
            "password": "samplepassword"
        }
    ]
}